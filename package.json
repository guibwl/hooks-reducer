{
  "name": "hooks-reducer",
  "version": "2.0.4",
  "description": "Library for reducer in React Hooks",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development rollup -m -c -o dist/hooks-reducer.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -m -c -o dist/hooks-reducer.min.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "keywords": [
    "hooks-reducer"
  ],
  "author": "guibwl",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "cross-env": "^7.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.1.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.0"
  }
}
